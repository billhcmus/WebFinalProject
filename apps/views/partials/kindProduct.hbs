<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">
            <strong>Chủng loại laptop</strong>
        </h3>
    </div>
    <div class="list-group">
        {{#each layoutVM.typesList}}
            <div class="listgroup-item click-box">  
                <a href="/shop/choose?att=type&id={{this.id}}"class="{{this.name}} classify"><input style="float:left;" type="checkbox" type="checkbox" class="{{this.name}} checkbox checkbox-btn" checked>&nbsp;{{this.name}}</a>
            </div>      
        {{/each}}
    </div>
</div>