<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">
            <strong>Hãng sản xuất</strong>
        </h3>
    </div>
    <div class="list-group">
        {{#each brandsList}}
        <div class="list-group-item click-box">
            <input type="checkbox" class="dell checkbox checkbox-btn" checked>
            <a style="text-decoration: none" href="/shop/brand?id={{this.id}}" class="{{this.name}} brand">{{this.name}}</a>
        </div>
        {{/each}}
    </div>
</div>